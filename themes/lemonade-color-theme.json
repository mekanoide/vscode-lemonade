    "editorGutter.foldingControlForeground": "#989898",
    "editorMarkerNavigationError.background": "#E94833",
    "editorMarkerNavigationWarning.background": "#d4c74f",
    "editorMarkerNavigationInfo.background": "#b48845",
    "editorMarkerNavigation.background": "#121212",
    "minimapGutter.addedBackground": "#4cda3f",
    "minimapGutter.deletedBackground": "#E94833",
    "minimapGutter.modifiedBackground": "#a6d65d",
    "minimap.selectionHighlight": "#d4c74f28",
    "minimap.errorHighlight": "#E94833",
    "minimap.warningHighlight": "#d4c74f",
    "minimap.infoHighlight": "#b48845"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "",
        "foreground": "#484848"
      }
    },
    {
      "name": "Types",
      "scope": [
        "storage.type",
        "storage.modifier",
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.as"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#aa9f5f"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#E2E2E2"
      }
    },
    {
      "name": "Variables properties",
      "scope": [
        "punctuation.separator.key-value",
        "keyword.operator.assignment"
        //        "keyword.operator.comparison"
        //				"punctuation.definition.string",
      ],
      "settings": {
        "foreground": "#989898"
      }
    },
    {
      "name": "Object property keys",
      "scope": ["meta.object-literal.key.ts", "meta.object-literal.key.js"],
      "settings": {
        "foreground": "#A8A8A8"
      }
    },
    {
      "name": "Functions and Tags",
      "scope": ["entity.name.function", "entity.name.tag", "entity.name.class"],
      "settings": {
        "foreground": "#d4c74f"
      }
    },
    {
      "name": "Punctuation and brackets",
      "scope": [
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.definition.typeparameters.begin",
        "meta.brace.round",
        "meta.function-call punctuation",
        "punctuation.definition.arguments",
        "punctuation.definition.entity",
        "punctuation.definition.type",
        "punctuation.section.scope",
        "string.template meta.brace"
      ],
      "settings": {
        "foreground": "#686868"
      }
    },
    {
      "name": "Strings",
      "scope": ["string"],
      "settings": {
        "foreground": "#c98151"
      }
    },
    {
      "name": "Attributes",
      "scope": ["meta.attribute"],
      "settings": {
        "foreground": "#c4c4c4"
      }
    },
    {
      "name": "HTML attributes",
      "scope": ["meta.attribute.class.html string", "text.html.derivative"],
      "settings": {
        "foreground": "#E2E2E2"
      }
    },
    {
      "name": "Operators and modifiers",
      "scope": [
        "support.type.primitive.ts",
        "keyword.control"
        //"keyword.operator",
        //"meta.attribute.directive"
      ],
      "settings": {
        "foreground": "#a6d65d"
      }
    },
    {
      "name": "Properties",
      "scope": [
        "support.variable.property.ts",
        "variable.other.property",
        "variable.other.object.property",
        "variable.other.property",
        "variable.other.object.property",
        "variable.other.constant.property",
        "meta.attribute.directive.vue"
      ],
      "settings": {
        "foreground": "#9bc45f"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        //"meta.var.expr",
        //"support.type.primitive",
        "meta.parameters.ts",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#9bc45f"
      }
    },
    // JSON
    {
      "name": "JSON keys",
      "scope": ["support.type.property-name.json.comments", "string.json"],
      "settings": {
        "foreground": "#d4c74f"
      }
    },
    {
      "name": "JSON values",
      "scope": ["string.quoted.double.json", "string.quoted.single.json"],
      "settings": {
        "foreground": "#E2E2E2"
      }
    },
    // CSS
    {
      "name": "CSS property values",
      "scope": ["meta.property-value.css"],
      "settings": {
        "foreground": "#c98151"
      }
    },
    {
      "name": "CSS rules",
      "scope": [
        "meta.selector.css",
        "entity.name.tag.css",
        "variable.parameter.keyframe-list.css"
      ],
      "settings": {
        "foreground": "#d4c74f"
      }
    },
    {
      "name": "CSS pseudoclasses",
      "scope": ["entity.other.attribute-name.pseudo-class.css"],
      "settings": {
        "foreground": "#a6d65d"
      }
    },
    {
      "name": "CSS properties",
      "scope": [
        "entity.other.attribute-name.value.css",
        "meta.at-rule.apply.tailwind",
        "meta.at-rule.layer.body.tailwind"
      ],
      "settings": {
        "foreground": "#E2E2E2"
      }
    },
    {
      "name": "CSS variables",
      "scope": ["meta.function.variable.css", "meta.function.color.css"],
      "settings": {
        "foreground": "#989898"
      }
    },
    {
      "name": "CSS custom properties",
      "scope": ["variable.argument.css", "entity.other.keyframe-offset.css"],
      "settings": {
        "foreground": "#a6d65d"
      }
    },
    // Markdown
    {
      "name": "Markdown links",
      "scope": [
        "string.other.link.title.markdown",
        "string.other.link.markdown"
      ],
      "settings": {
        "foreground": "#a6d65d"
      }
    },
    {
      "name": "Markdown titles",
      "scope": ["markup.heading.markdown"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#E2E2E2"
      }
    },
    {
      "name": "Markdown code",
      "scope": ["markup.fenced_code.block.markdown"],
      "settings": {
        "foreground": "#989898"
      }
    },
    {
      "name": "Typescript operators",
      "scope": ["keyword.operator.type.annotation"],
      "settings": {
        "foreground": "#989898"
      }
    }
  ]
}
