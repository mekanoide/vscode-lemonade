{
  "name": "Omar Noir",
  "type": "dark",
  "colors": {
    "editor.background": "#121212",
    "editor.foreground": "#e8e8e8",
    "selection.background": "#EBDA47",
    "editor.selectionBackground": "#98989848",
    "editor.lineHighlightBorder": "#989898",
    "editor.lineHighlightBackground": "#121212",
    "editorLineNumber.activeForeground": "#EBDA47",
    "editorLineNumber.foreground": "#484848",
    "editorCursor.background": "#EBDA47",
    "editorCursor.foreground": "#EBDA47",
    "focusBorder": "#484848",
    "textLink.foreground": "#98e824",
    "progressBar.background": "#EBDA47",
    "titleBar.activeBackground": "#121212",
    "titleBar.activeForeground": "#989898",
    "titleBar.inactiveBackground": "#121212",
    "titleBar.border": "#242424",
    "activityBar.background": "#121212",
    "activityBar.foreground": "#EBDA47",
    "activityBar.border": "#242424",
    "activityBar.activeBorder": "#EBDA47",
    "activityBar.inactiveForeground": "#484848",
    "activityBarBadge.background": "#EBDA47",
    "activityBarBadge.foreground": "#121212",
    "sideBarTitle.foreground": "#989898",
    "sideBar.background": "#121212",
    "sideBar.foreground": "#989898",
    "sideBar.border": "#242424",
    "sideBarSectionHeader.background": "#121212",
    "sideBarSectionHeader.border": "#242424",
    "statusBar.background": "#121212",
    "statusBar.foreground": "#e8e8e8",
    "statusBar.noFolderBackground": "#121212",
    "statusBar.border": "#242424",
    "statusBar.debuggingBackground": "#EB8947",
    "statusBar.debuggingForeground": "#121212",
    "tree.indentGuidesStroke": "#484848",
    "tab.activeForeground": "#e8e8e8",
    "tab.inactiveBackground": "#121212",
    "tab.inactiveForeground": "#989898",
    "tab.activeBorder": "#EBDA47",
    "editorGroupHeader.tabsBackground": "#121212",
    "editorGroupHeader.tabsBorder": "#242424",
    "editorGroup.dropBackground": "#242424",
    "editorGroup.border": "#242424",
    "editorGroupHeader.border": "#242424",
    "editorGroupHeader.noTabsBackground": "#121212",
    "button.background": "#EBDA47",
    "button.foreground": "#121212",
    "button.secondaryBackground": "#484848",
    "button.secondaryForeground": "#e8e8e8",
    "breadcrumb.background": "#121212",
    "breadcrumb.activeSelectionForeground": "#e8e8e8",
    "input.background": "#242424",
    "input.foreground": "#e8e8e8",
    "editorBracketHighlight.foreground1": "#989898", // Primer nivel
    "editorBracketHighlight.foreground2": "#989898", // Segundo nivel
    "editorBracketHighlight.foreground3": "#989898", // Tercer nivel
    "editorBracketHighlight.foreground4": "#989898", // Cuarto nivel
    "editorBracketHighlight.foreground5": "#989898", // Quinto nivel
    "editorBracketHighlight.foreground6": "#989898", // Sexto nivel
    "editorBracketHighlight.unexpectedBracket.foreground": "#989898",
    "editorError.foreground": "#e94833",
    "editorWarning.foreground": "#EBDA47",
    "list.inactiveSelectionForeground": "#e8e8e8",
    "list.inactiveSelectionIconForeground": "#e8e8e8",
    "list.inactiveSelectionBackground": "#242424",
    "list.activeSelectionIconForeground": "#121212",
    "list.activeSelectionBackground": "#EBDA47",
    "list.activeSelectionForeground": "#121212",
    "list.hoverBackground": "#242424",
    "list.hoverForeground": "#e8e8e8",
    "list.deemphasizedForeground": "#686868",
    "list.highlightForeground": "#EBDA47",
    "gitDecoration.addedResourceForeground": "#A4EB47",
    "gitDecoration.modifiedResourceForeground": "#C9D65A",
    "gitDecoration.deletedResourceForeground": "#EB8947",
    "gitDecoration.untrackedResourceForeground": "#b48845",
    "gitDecoration.ignoredResourceForeground": "#686868",
    "gitDecoration.conflictingResourceForeground": "#EBDA47",
    "gitDecoration.submoduleResourceForeground": "#e8e8e8",
    "gitDecoration.stageDeletedResourceForeground": "#EB8947",
    "gitDecoration.stageModifiedResourceForeground": "#C9D65A",
    "menu.background": "#121212",
    "menu.foreground": "#e8e8e8",
    "menu.border": "#242424",
    "menu.selectionBackground": "#242424",
    "menu.selectionForeground": "#e8e8e8",
    "menu.selectionBorder": "#242424",
    "menu.separatorBackground": "#242424",
    "editorWidget.background": "#121212",
    "editorWidget.foreground": "#e8e8e8",
    "editorWidget.border": "#484848",
    "editorWidget.resizeBorder": "#EBDA47"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "",
        "foreground": "#686868"
      }
    },
    {
      "name": "Types",
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#989898"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#e8e8e8"
      }
    },
    {
      "name": "Variables properties",
      "scope": [
        "punctuation.separator.key-value",
        "keyword.operator.assignment"
        //        "keyword.operator.comparison"
        //				"punctuation.definition.string",
      ],
      "settings": {
        "foreground": "#989898"
      }
    },
    {
      "name": "Functions and Tags",
      "scope": ["entity.name.function", "entity.name.tag"],
      "settings": {
        "foreground": "#EBDA47"
      }
    },
    {
      "name": "Punctuation and brackets",
      "scope": [
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.definition.typeparameters.begin",
        "meta.brace.round",
        "meta.function-call punctuation",
        "punctuation.definition.arguments",
        "punctuation.definition.entity",
        "punctuation.definition.type",
        "punctuation.section.scope",
        "string.template meta.brace"
      ],
      "settings": {
        "foreground": "#989898"
      }
    },
    {
      "name": "Imports and exports",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.as"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#a8a8a8"
      }
    },
    {
      "name": "Strings",
      "scope": ["string"],
      "settings": {
        "foreground": "#EB8947"
      }
    },
    {
      "name": "Attributes",
      "scope": ["meta.attribute"],
      "settings": {
        "foreground": "#c4c4c4"
      }
    },
    {
      "name": "Expressions",
      "scope": [
        "storage.type",
        "keyword.control",
        "storage.modifier",
        "keyword.operator",
        "meta.attribute.directive"
      ],
      "settings": {
        "foreground": "#A4EB47"
      }
    },
    {
      "name": "Properties",
      "scope": [
        "support.variable.property.ts",
        "variable.other.property",
        "variable.other.object.property",
        "variable.other.property",
        "variable.other.object.property",
        "variable.other.constant.property"
      ],
      "settings": {
        "foreground": "#C9D65A"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        //"meta.var.expr",
        //"support.type.primitive",
        "meta.parameters.ts",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#98e824"
      }
    },
    // JSON
    {
      "name": "JSON keys",
      "scope": ["support.type.property-name.json.comments", "string.json"],
      "settings": {
        "foreground": "#EBDA47"
      }
    },
    {
      "name": "JSON values",
      "scope": ["string.quoted.double.json", "string.quoted.single.json"],
      "settings": {
        "foreground": "#e8e8e8"
      }
    },
    // CSS
    {
      "name": "CSS property values",
      "scope": [
        "meta.property-value.css",
      ],
      "settings": {
        "foreground": "#EB8947"
      }
    },
    {
      "name": "CSS rules",
      "scope": [
        "meta.selector.css",
        "entity.name.tag.css",
        "variable.parameter.keyframe-list.css",
      ],
      "settings": {
        "foreground": "#EBDA47"
      }
    },
    {
      "name": "CSS pseudoclasses",
      "scope": ["entity.other.attribute-name.pseudo-class.css"],
      "settings": {
        "foreground": "#C9D65A"
      }
    },
    {
      "name": "CSS properties",
      "scope": [
        "entity.other.attribute-name.value.css",
        "meta.at-rule.apply.tailwind",
        "meta.at-rule.layer.body.tailwind"
      ],
      "settings": {
        "foreground": "#e8e8e8"
      }
    },
    {
      "name": "CSS variables",
      "scope": ["meta.function.variable.css"],
      "settings": {
        "foreground": "#989898"
      }
    },
    {
      "name": "CSS custom properties",
      "scope": ["variable.argument.css", "entity.other.keyframe-offset.css"],
      "settings": {
        "foreground": "#98e824"
      }
    },
    // Markdown
    {
      "name": "Markdown links",
      "scope": [
        "string.other.link.title.markdown",
        "string.other.link.markdown"
      ],
      "settings": {
        "foreground": "#98e824"
      }
    },
    {
      "name": "Markdown titles",
      "scope": ["markup.heading.markdown"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#e8e8e8"
      }
    },
    {
      "name": "Markdown code",
      "scope": ["markup.fenced_code.block.markdown"],
      "settings": {
        "foreground": "#989898"
      }
    },
    {
      "name": "Typescript operators",
      "scope": ["keyword.operator.type.annotation"],
      "settings": {
        "foreground": "#989898"
      }
    }
  ]
}
